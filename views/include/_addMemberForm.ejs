<!-- form handling -->
<form action="/api/member" method="POST" id="add_member">
    <div class="new_member">
        <div class="form-group">
            <label for="name" class="text-light">Name</label>
            <input type="hidden" name="id" value="">
            <input type="text" name="name" value="" placeholder="Mark Stoenis">
        </div>
        <div class="form-group">
            <label for="address" class="text-light">Address</label>
            <input type="text" name="address" value="" placeholder="Mountain View, California">
        </div>
        <div class="form-group">
            <label for="phoneNumber" class="text-light">Phone Number</label>
            <input type="text" name="phoneNumber" value="" placeholder="91 XXXXXXXXXX">
        </div>
        <div class="form-group">
            <label for="email" class="text-light">Email</label>
            <input type="text" name="email" value="" placeholder="example@gmail.com">
        </div>

        <div class="form-group">
            <button type="submit" class="btn text-dark update">Save</button>
        </div>

    </div>
</form>